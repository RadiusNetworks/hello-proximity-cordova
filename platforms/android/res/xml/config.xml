<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.hello" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidLaunchMode" value="singleTop" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="ProximityKit">
        <param name="android-package" value="com.radiusnetworks.cordova.proximity.ProximityKitPlugin" />
    </feature>
    <name>HelloWorld</name>
    <description>
        A sample Apache Cordova application that works with ProximityKit.
    </description>
    <author email="dev@example.com">
        Example PhoneGap developer
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
